  "version": "0.2.0",
  "configurations": [
  {
    "name": "QML stoat",
    "type": "cppdbg",
    "request": "launch",
    "program": "${workspaceFolder}/clients/stoat_chat_qml_kirigami/build/bin/stoat_chat_qml_kirigami",
    "args": [],
    "stopAtEntry": false,
    "cwd": "${workspaceFolder}/clients/stoat_chat_qml_kirigami",
    "environment": [],
    "externalConsole": false,
    "MIMode": "gdb",
    "setupCommands": [
        {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        },
        {
            "description":  "Set Disassembly Flavor to Intel",
            "text": "-gdb-set disassembly-flavor intel",
            "ignoreFailures": true
        }
    ]
  },

    {
      "name": "debug iodine-nodb",
      "type": "Ruby",
      "request": "launch",
      "program": "${workspaceRoot}/servers/ruby-iodine-nodb/start.rb"
    }
  ]
}